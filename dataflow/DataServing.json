					},
					"name": "sinkGold2"
				},
				{
					"linkedService": {
						"referenceName": "linkedservice_datalake",
						"type": "LinkedServiceReference"
					},
					"name": "sinkGold3"
				}
			],
			"transformations": [
				{
					"name": "AirlineJoin"
				},
				{
					"name": "selectCols2"
				},
				{
					"name": "AggregateAirline"
				},
				{
					"name": "Ranking"
				},
				{
					"name": "filterTop5"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "FlightJoin"
				},
				{
					"name": "selectCols1"
				},
				{
					"name": "AggregateFlight"
				},
				{
					"name": "windowRanking"
				},
				{
					"name": "filterTopFive"
				},
				{
					"name": "alterRow2"
				},
				{
					"name": "PassengerJoin"
				},
				{
					"name": "selectCols3"
				},
				{
					"name": "AggregatePassenger"
				},
				{
					"name": "UpdatingNULLS"
				},
				{
					"name": "Rankingwindow"
				},
				{
					"name": "AvoidNULLs"
				},
				{
					"name": "filter1"
				},
				{
					"name": "alterRow3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          airline_id as short,",
				"          airline_name as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimAirline') ~> DimAirline",
				"source(output(",
				"          flight_id as short,",
				"          flight_number as string,",
				"          departure_timestamp as string,",
				"          arrival_timestamp as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimFlight') ~> DimFlight",
				"source(output(",
				"          booking_id as integer,",
				"          passenger_id as integer,",
				"          flight_id as integer,",
				"          airline_id as integer,",
				"          origin_airport_id as integer,",
				"          destination_airport_id as integer,",
				"          booking_date as date,",
				"          ticket_cost as integer,",
				"          flight_duration_mins as integer,",
				"          checkin_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'FactBookings') ~> FactBookings",
				"source(output(",
				"          passenger_id as short,",
				"          full_name as string,",
				"          gender_flag as string,",
				"          age as short,",
				"          country as string,",
				"          first_name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimPassenger') ~> DimPassenger",
				"FactBookings, DimAirline join(FactBookings@airline_id == DimAirline@airline_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> AirlineJoin",
				"AirlineJoin select(mapColumn(",
				"          booking_id,",
				"          passenger_id,",
				"          flight_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status,",
				"          airline_id = DimAirline@airline_id,",
				"          airline_name,",
				"          country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectCols2",
				"selectCols2 aggregate(groupBy(airline_name),",
				"     Total_Sales = sum(ticket_cost)) ~> AggregateAirline",
				"AggregateAirline window(desc(Total_Sales, true),",
				"     TopRanking = denseRank()) ~> Ranking",
				"Ranking filter(TopRanking < 6) ~> filterTop5",
				"filterTop5 alterRow(insertIf(1==1)) ~> alterRow1",
				"FactBookings, DimFlight join(FactBookings@flight_id == DimFlight@flight_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> FlightJoin",
				"FlightJoin select(mapColumn(",
				"          booking_id,",
				"          passenger_id,",
				"          flight_id = FactBookings@flight_id,",
				"          airline_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status,",
				"          flight_number,",
				"          departure_timestamp,",
				"          arrival_timestamp",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectCols1",
				"selectCols1 aggregate(groupBy(flight_number),",
				"     Total_Cost = sum(ticket_cost)) ~> AggregateFlight",
				"AvoidNULLs window(desc(Total_Cost, true),",
				"     TopList = denseRank()) ~> windowRanking",
				"windowRanking filter(TopList < 6) ~> filterTopFive",
				"filterTopFive alterRow(insertIf(1==1)) ~> alterRow2",
				"FactBookings, DimPassenger join(FactBookings@passenger_id == DimPassenger@passenger_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> PassengerJoin",
				"PassengerJoin select(mapColumn(",
				"          booking_id,",
				"          passenger_id = FactBookings@passenger_id,",
				"          flight_id,",
				"          airline_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status,",
				"          full_name,",
				"          gender_flag,",
				"          age,",
				"          country,",
				"          first_name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectCols3",
				"selectCols3 aggregate(groupBy(country),",
				"     ticket_cost = sum(ticket_cost)) ~> AggregatePassenger",
				"AggregatePassenger derive(country = coalesce(country,'AroundTheWorld')) ~> UpdatingNULLS",
				"UpdatingNULLS window(asc(country, true),",
				"     TopCountry = denseRank()) ~> Rankingwindow",
				"AggregateFlight derive(flight_number = coalesce(flight_number,'ALLAIRLINES')) ~> AvoidNULLs",
				"Rankingwindow filter(TopCountry < 6) ~> filter1",
				"filter1 alterRow(insertIf(0 == 0 )) ~> alterRow3",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'gold',",
				"     folderPath: 'businessView',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     keys:['airline_name'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkGold1",
				"alterRow2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'gold',",
				"     folderPath: 'businessView1',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkGold2",
				"alterRow3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'gold',",
				"     folderPath: 'businessView2',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkGold3"
			]
		}
	}
}