{
	"name": "DataServing",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "linkedservice_datalake",
						"type": "LinkedServiceReference"
					},
					"name": "DimAirline"
				},
				{
					"linkedService": {
						"referenceName": "linkedservice_datalake",
						"type": "LinkedServiceReference"
					},
					"name": "DimFlight"
				},
				{
					"linkedService": {
						"referenceName": "linkedservice_datalake",
						"type": "LinkedServiceReference"
					},
					"name": "FactBookingsAirline"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "linkedservice_datalake",
						"type": "LinkedServiceReference"
					},
					"name": "sinkGold1"
				},
				{
					"linkedService": {
						"referenceName": "linkedservice_datalake",
						"type": "LinkedServiceReference"
					},
					"name": "sinkGold2"
				}
			],
			"transformations": [
				{
					"name": "AirlineJoin"
				},
				{
					"name": "selectCols2"
				},
				{
					"name": "AggregateAirline"
				},
				{
					"name": "Ranking"
				},
				{
					"name": "filterTop5"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "FactBookingjoin"
				},
				{
					"name": "selectCols1"
				},
				{
					"name": "AggregateFlight"
				},
				{
					"name": "windowRanking"
				},
				{
					"name": "derivedColumnNULLS"
				},
				{
					"name": "filterTopFive"
				},
				{
					"name": "alterRowSample"
				}
			],
			"scriptLines": [
				"source(output(",
				"          airline_id as short,",
				"          airline_name as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimAirline') ~> DimAirline",
				"source(output(",
				"          flight_id as short,",
				"          flight_number as string,",
				"          departure_timestamp as string,",
				"          arrival_timestamp as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimFlight') ~> DimFlight",
				"source(output(",
				"          booking_id as integer,",
				"          passenger_id as integer,",
				"          flight_id as integer,",
				"          airline_id as integer,",
				"          origin_airport_id as integer,",
				"          destination_airport_id as integer,",
				"          booking_date as date,",
				"          ticket_cost as integer,",
				"          flight_duration_mins as integer,",
				"          checkin_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'FactBookings') ~> FactBookingsAirline",
				"FactBookingsAirline, DimAirline join(FactBookingsAirline@airline_id == DimAirline@airline_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> AirlineJoin",
				"AirlineJoin select(mapColumn(",
				"          booking_id,",
				"          passenger_id,",
				"          flight_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status,",
				"          airline_id = DimAirline@airline_id,",
				"          airline_name,",
				"          country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectCols2",
				"selectCols2 aggregate(groupBy(airline_name),",
				"     Total_Sales = sum(ticket_cost)) ~> AggregateAirline",
				"AggregateAirline window(desc(Total_Sales, true),",
				"     TopRanking = denseRank()) ~> Ranking",
				"Ranking filter(TopRanking < 6) ~> filterTop5",
				"filterTop5 alterRow(insertIf(1==1)) ~> alterRow1",
				"FactBookingsAirline, DimFlight join(FactBookingsAirline@flight_id == DimFlight@flight_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> FactBookingjoin",
				"FactBookingjoin select(mapColumn(",
				"          booking_id,",
				"          passenger_id,",
				"          flight_id = FactBookingsAirline@flight_id,",
				"          airline_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status,",
				"          flight_number,",
				"          departure_timestamp,",
				"          arrival_timestamp",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectCols1",
				"selectCols1 aggregate(groupBy(flight_number),",
				"     Total_Cost = sum(ticket_cost)) ~> AggregateFlight",
				"AggregateFlight window(desc(Total_Cost, true),",
				"     TopList = denseRank()) ~> windowRanking",
				"windowRanking derive(flight_number = coalesce(flight_number,\"AIR789\")) ~> derivedColumnNULLS",
				"derivedColumnNULLS filter(TopList < 6) ~> filterTopFive",
				"filterTopFive alterRow(insertIf(1==1 )) ~> alterRowSample",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'gold',",
				"     folderPath: 'businessView',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     keys:['airline_name'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkGold1",
				"alterRowSample sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'gold',",
				"     folderPath: 'businessView1',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkGold2"
			]
		}
	}
}